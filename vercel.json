{
  "version": 2,
  "builds": [
    {
      "src": "Frontend/mtdna-project/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Frontend/mtdna-project/dist"
      }
    },
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/mtdna-project/dist/index.html"
    }
  ]
}